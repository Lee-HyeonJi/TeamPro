<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
        <setting name="cacheEnabled" value="false" />
        <setting name="jdbcTypeForNull" value="NULL" />
    </settings>
	<typeAliases>
		<typeAlias type="com.dogcompany.dogapp.model.MemberDTO" alias="memberDTO"/>
		<typeAlias type="com.dogcompany.dogapp.model.CareDTO" alias="careDTO"/>
		<typeAlias alias="recommendaionDTO" type="com.dogcompany.dogapp.model.RecommendationDTO"/>
		<typeAlias alias="photoDTO" type="com.dogcompany.dogapp.model.PhotoDTO"/>
	</typeAliases>

	<mappers>
	  <mapper resource="mybatis/mapper/dogapp.xml"/>
	  <mapper resource="mybatis/mapper/backend.xml"/>
	</mappers>
  

</configuration>